{
  "env": {
    "browser": true,
    "node": true
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "plugins": [
    "react"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "react/prop-types": "off",
    "no-unused-vars": "error",
    "no-console": ["warn", { "allow": ["warn", "error"] }]
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"]
}
